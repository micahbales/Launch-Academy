<h1><%= @meetup.title %></h1>

<div id="meetup-details">
<h3>Location: <%= @meetup.location %></h3>
<h3>Description: <%= @meetup.description %></h3>
<h3>Creator: <%= @meetup.creator %></h3>
<form action="/meetups/<%= @meetup.id %>" method="post">
<button id="join_meetup">Join</button>
</div>

<div id="user-list">
  <ul>
    <% @users.each do |user| %>
      <li><img class="avatar-small" src="<%= user.avatar_url %>"> <%= user.username %></li>
    <% end %>
  </ul>
</div>
