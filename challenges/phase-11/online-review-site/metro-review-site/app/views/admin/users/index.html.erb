<h1>All Users</h1>

<ul>
  <% @users.order(first_name: :desc).each do |user| %>

    <li id="user-<%= user.id %>"><%= user.first_name %> <%= user.last_name %> (<%= link_to "Delete User", admin_user_path(user), method: :delete, id: "user-delete-#{user.id}", class: "user-delete" %>)</li>
  <% end %>
</ul>
